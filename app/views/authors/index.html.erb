<h1>Authors#index</h1>
<p>Find me in app/views/authors/index.html.erb</p>


<table>
	<tr>
		<th>Author Name</th>
		<th>Actions</th>
	</tr>
	<% @authors.each do |author| %>
		<tr>
			<td><%= author.first_name %></td>
			<td>
				<%= link_to("Show", author_path(author)) %>
				<%= link_to("Edit", edit_author_path(author)) %>
				<%= link_to("Delete", delete_author_path(author)) %>
			</td>
		</tr>
	<% end %>
</table>