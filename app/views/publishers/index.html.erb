<!-- <h2>List of Publishers</h2> -->

<table class="table table-hover">
<!-- <table class="table table-responsive">  -->
	<tr>
		<th>Name</th>
		<th>Actions</th>
	</tr>
	<% @publishers.each do |publisher| %>
		<tr>
			<td><%= publisher.name %></td>
			<td>
				<%= link_to("Show", publisher_path(publisher)) %> |
				<%= link_to("Edit", edit_publisher_path(publisher)) %> |
				<%= link_to("Delete", delete_publisher_path(publisher)) %>
			</td>
		</tr>
	<% end %>
</table>

<%= link_to("New Publisher", new_publisher_path , :class => 'btn btn-primary', ) %>




